[global_config]
  extra_styling = False
  tab_position = hidden
  title_use_system_font = False
  smart_copy = False
[keybindings]
  split_horiz = <Ctrl><Shift>R
  split_vert = <Ctrl><Shift>E
  close_term = <Ctrl><Shift>W
  copy = <Ctrl><Shift>C
  paste = <Ctrl><Shift>V
  move_tab_right = <Ctrl><Shift>Right
  move_tab_left = <Ctrl><Shift>Left
  reset = <Ctrl><Shift>J
[profiles]
  [[default]]
    background_darkness = 0.7
    background_type = transparent
    cursor_shape = ibeam
    cursor_color = "#aaaaaa"
    show_titlebar = False
    scrollbar_position = disabled
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
